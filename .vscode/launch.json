   "version": "0.2.0",
   "configurations": [
     {
      "name": "Launch development",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/workbook", 
      "program": "lib/main_development.dart",
      "args": [
         "--flavor",
         "development",
         "--target",
         "lib/main_development.dart"
      ]
     },
     {
      "name": "Launch production (debug)",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/workbook", 
      "program": "lib/main_production.dart",
      "args": [
         "--flavor", 
         "production", 
         "--target", 
         "lib/main_production.dart"
      ],
      "flutterMode": "debug",
     },
     {
      "name": "Launch production (release)",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/workbook", 
      "program": "lib/main_production.dart",
      "args": [
         "--flavor", 
         "production", 
         "--target", 
         "lib/main_production.dart"
      ],
      "flutterMode": "release",
     },
     {
      "name": "Launch RuStore development",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/workbook",
      "program": "lib/main_rustore_development.dart",
      "args": [
         "--flavor",
         "development",
         "--target",
         "lib/main_rustore_development.dart"
      ],
      "flutterMode": "release",
     },
     {
      "name": "Launch RuStore production (release)",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/workbook",  
      "program": "lib/main_rustore_production.dart",
      "args": [
         "--flavor", 
         "production", 
         "--target", 
         "lib/main_rustore_production.dart"
      ],
      "flutterMode": "release",
     }
   ]
 }
 